<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="OFF">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%highlight{%d{yyyy-MM-dd} %d{HH:mm:ss} %-5level %-27C{1} - [%M] %msg%n}"/>
        </Console>

    </Appenders>
    <Loggers>
        <Root level="ALL">
            <AppenderRef ref="Console" level="OFF"/>
        </Root>
    </Loggers>
</Configuration>